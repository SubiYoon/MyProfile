<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="profile.introduce.myself.main.mapper.MainMapper">
    <select id="selectMyInfo" resultType="SampleVo">
        SELECT
            NAME
            , BIRTH_YEAR
            , SEX
            , ADDR
            , ADDR_DETAIL
            , SIMPLE_INTRODUCE_MYSELF
            , DETAIL_INTRODUCE_MYSELF
            , IMAGE
        FROM MY_PROFILE
    </select>
</mapper>